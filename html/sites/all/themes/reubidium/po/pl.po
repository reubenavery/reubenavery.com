#: theme-settings.inc:71
msgid "Page alignment"
msgstr "Wyrównanie strony"

#: theme-settings.inc:74
msgid "left"
msgstr "lewo"

#: theme-settings.inc:75
msgid "center"
msgstr "środkowo"

#: theme-settings.inc:76
msgid "right"
msgstr "prawo"

#: theme-settings.inc:78
msgid "Align the page to right, center or left. Default: \"@default\"."
msgstr "Wyrównuje strone na prawo, środkowo albo lewo. Standard: \"@default\"."

#: theme-settings.inc:82
msgid "Page width"
msgstr "szerokość strony"

#: theme-settings.inc:84
msgid "Defines the fixed page width in <em>px</em> or <em>em</em>. Default: \"@default\"."
msgstr "Definuje stałą szerokość strony w <em>px</em> or <em>em</em>. Standard: \"@default\"."

#: theme-settings.inc:90
msgid "Page min-width"
msgstr "Minimalna szerokość strony"

#: theme-settings.inc:92
msgid "Defines the minium page width in <em>px</em> or <em>em</em>. Default: \"@default\"."
msgstr "Definuje minimalną szerokość w <em>px</em> albo <em>em</em>. Standard: \"@default\"."

#: theme-settings.inc:98
msgid "Page max-width"
msgstr "Maksymalna szerokość strony"

#: theme-settings.inc:100
msgid "Defines the maximum page width in <em>px</em> or <em>em</em>. Default: \"@default\"."
msgstr "Definuje maksymalną szerokość strony w <em>px</em> albo <em>em</em>. Standard: \"@default\"."

#: theme-settings.inc:108
msgid "Left sidebar"
msgstr "Lewy pasek boczny"

#: theme-settings.inc:110
msgid "Defines the left column width in <em>px</em>, <em>em</em> or <em>%</em>. Default: \"@default\"."
msgstr "Definuje maksymalną szerokość strony w <em>px</em>, <em>em</em> albo <em>%</em>. Standard: \"@default\"."

#: theme-settings.inc:118
msgid "Content"
msgstr "Zawartość"

#: theme-settings.inc:130
msgid "Right sidebar"
msgstr "Prawy pasek boczny"

#: theme-settings.inc:132
msgid "Defines the right column width in <em>px</em>, <em>em</em> or <em>%</em>. Default: \"@default\"."
msgstr "Definuje maksymalną szerokość strony w <em>px</em>, <em>em</em> albo <em>%</em>. Standard: \"@default\"."

