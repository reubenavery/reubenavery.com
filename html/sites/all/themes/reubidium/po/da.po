msgstr "Definerer den maksimale side bredde i <em>px</em> eller <em>em</em>. Standard: \"@default\"."

#: theme-settings.inc:108
msgid "Left sidebar"
msgstr "Venstre sidepanel"

#: theme-settings.inc:110
msgid "Defines the left column width in <em>px</em>, <em>em</em> or <em>%</em>. Default: \"@default\"."
msgstr "Definerer den maksimale side bredde i <em>px</em>, <em>em</em> eller <em>%</em>. Standard: \"@default\"."

